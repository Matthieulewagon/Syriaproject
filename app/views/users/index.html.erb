<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |doctor| %>
      <tr>
        <td><%= link_to "#{doctor.username}", user_path(doctor) %></td>
        <td><%= doctor.last_name %></td>
        <td><%= doctor.address %></td>

      </tr>
    <% end %>
  </tbody>
</table>
